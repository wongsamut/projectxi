@charset "utf-8";
@navbar-bg-color: #AC193D;
@navbar-bg-hover-color: #4B4B4B;
@navbar-bg-dropdown-color: #FAFAFA;
@navbar-bg-dropdown-hover-color: #4B4B4B;
@navbar-bg-iconbar: white;
@navbar-border-dropdown-color: #4B4B4B;
@navbar-border-color: #AC193D;
@navbar-divider-color: #4B4B4B;
@navbar-text-color: white;
@navbar-dropdown-text-color: black;
@navbar-dropdown-hover-text-color: white;

div.pack
{
	padding-left: 0px !important;
	padding-right: 0px !important;
}

.navbar-toggle .icon-bar
{
    background-color: @navbar-bg-iconbar !important;
}

.navbar-background
{
	background-color: @navbar-bg-color;
}

.navbar-default
{
	background-color: @navbar-bg-color !important;
	border-color: @navbar-border-color !important;
}

.navbar-default .navbar-brand
{
	color: @navbar-text-color !important;
}

.navbar-default .navbar-nav>li>a
{
	color: @navbar-text-color !important;
}

.navbar-default .navbar-nav>li>a:hover
{
	background-color: @navbar-bg-hover-color !important;
}

.navbar-default .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:hover, .navbar-default .navbar-nav>.open>a:focus
{
	background-color: @navbar-bg-hover-color !important;
}

.dropdown-menu
{
	background-color: @navbar-bg-dropdown-color !important;
	border-color: @navbar-border-dropdown-color !important;
}

.dropdown-menu>li>a
{
	color: @navbar-dropdown-text-color !important;
}

.dropdown-menu>li>a:hover
{
	color: @navbar-dropdown-hover-text-color !important;
	background-color: @navbar-bg-dropdown-hover-color !important;
}

.dropdown-menu .divider
{
	background-color: @navbar-divider-color !important;
}


